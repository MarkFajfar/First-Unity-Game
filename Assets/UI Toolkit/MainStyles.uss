    color: rgb(179, 51, 40);
    font-size: 64px;
    border-left-color: rgb(151, 64, 27);
    border-right-color: rgb(151, 64, 27);
    border-top-color: rgb(151, 64, 27);
    border-bottom-color: rgb(151, 64, 27);
    border-left-width: 8px;
