.Headline {
    -unity-font: resource('Fonts/EBGaramond-Medium');
    -unity-font-definition: resource('Fonts/EBGaramond-Medium SDF');
    color: rgb(179, 51, 40);
    -unity-text-align: upper-center;
    font-size: 100px;
    flex-wrap: wrap;
    top: 5%;
    position: absolute;
}

.ButtonMenu {
    width: 680px;
    height: 110px;
    -unity-font: resource('Fonts/EBGaramond-Medium');
    -unity-font-definition: resource('Fonts/EBGaramond-Medium SDF');
    background-color: rgba(55, 29, 12, 0.78);
    color: rgb(179, 51, 40);
    font-size: 64px;
    border-left-color: rgb(179, 51, 40);
    border-right-color: rgb(179, 51, 40);
    border-top-color: rgb(179, 51, 40);
    border-bottom-color: rgb(179, 51, 40);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 24px;
    border-bottom-left-radius: 24px;
    border-top-right-radius: 24px;
    border-bottom-right-radius: 24px;
    margin-left: 24px;
    margin-right: 24px;
    margin-top: 24px;
    margin-bottom: 24px;
    align-items: center;
}

.ButtonMenu:hover {
    width: 750px;
    height: 110px;
    -unity-font: resource('Fonts/EBGaramond-Medium');
